                            <div class="row clearfix">



                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.company_name, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">اسم الشركة المصنعة </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.HiddenFor(x => x.id)


                                            @Html.EditorFor(x => x.naemcarton, new { HtmlAttributes = new { @class = "form-control", type = "text" } })


                                            <label class="form-label">اسم الورق او الكرتون </label>
                                        </div>
                                    </div>
                                </div>













                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.wakilmahalt, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">وكيل محلي </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.wakelalami, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">وكيل عالمي  </label>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.numpermwasafh, new { HtmlAttributes = new { @class = "form-control", type = "number" } })
                                            <label class="form-label">رقم المواصفة  </label>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.contry, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">بلد المنشا </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.refec, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">ref </label>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.phone_Supplier, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">رقم المورد </label>
                                        </div>
                                    </div>
                                </div>



                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.Email, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">الايميل </label>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.website, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">موقع الشركة  </label>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.numgemshet, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
