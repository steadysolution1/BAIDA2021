                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            @Html.EditorFor(x => x.Note, new { HtmlAttributes = new { @class = "form-control", type = "text" } })
                                            <label class="form-label">ملاحظات </label>
                                        </div>
                                    </div>
                                </div>


                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">

                                    </div>
                                </div>


                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">

                                    </div>
                                </div>
                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">

                                    </div>
                                </div>


                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">


                                    <div class="file-field input-field">
                                        <div class="btn">
                                            <span>المواصفة</span>
                                            <input type="file" name="upload">
                                        </div>
                                        <div class="file-path-wrapper">
                                            <input class="file-path validate" type="text">
                                        </div>
                                    </div>
                                </div>
















                                @Html.HiddenFor(x => x.idtypcoton, new { @Value = ViewData["id"] })










                                <div class="col-lg-3 col-md-3 col-sm-3 col-xs-6">
                                    <div class="form-group form-float">
                                        <div class="form-line">
                                            <button type="submit" class="btn btn-success waves-effect">حفظ </button>

                                        </div>
                                    </div>
                                </div>







