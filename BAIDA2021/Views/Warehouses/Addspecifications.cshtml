}

<head>

    <!-- Colorpicker Css -->
    <!-- Multi Select Css -->
    <link href="~/rtl/assets/js/bundles/multiselect/css/multi-select.css" rel="stylesheet">
    <!-- Plugins Core Css -->
    <link href="~/rtl/assets/css/app.min.css" rel="stylesheet">
    <link href="~/rtl/assets/js/bundles/materialize-rtl/materialize-rtl.min.css" rel="stylesheet">
    <link href="~/rtl/assets/css/form.min.css" rel="stylesheet">
    <!-- Custom Css -->
    <link href="~/rtl/assets/css/style.css" rel="stylesheet">
    <!-- You can choose a theme from css/styles instead of get all themes -->
    <link href="~/rtl/assets/css/styles/all-themes.css" rel="stylesheet" />
</head>
<style>
    #delevary_Place {
        display: block;
    }
</style>
<section class="content">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style ">
                        <li class="breadcrumb-item">
                            <h4 class="page-title">المستودعات</h4>
                        </li>
                        <li class="breadcrumb-item bcrumb-1">
                            <a href="#">
                                <i class="fas fa-home"></i>اضافة مواصفة


                            </a>
                        </li>

                    </ul>
                </div>
            </div>
        </div>
        <div class="alert alert-warning alert-dismissible" role="alert">
            <button type="button" class="close" data-dismiss="alert" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
           @ViewData["nametype"] : النوع الكرتون 
        </div>

        <div class="block-header">
            <div class="row">
                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                    <ul class="breadcrumb breadcrumb-style ">
                        <li class="breadcrumb-item">
                            <h4 class="page-title"></h4>
                        </li>


                    </ul>
                </div>
            </div>
        </div>

